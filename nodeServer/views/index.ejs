<html>
    <head>
        <meta charset="utf-8">
    </head>

    <style>
        table{
            font-family: sans-serif;
            border-collapse: collapse;
            width: 100%;
        }
        td, th{
            border: 1px solid grey;
            text-align: left;
            padding: 8px;
        }
    </style>

    <body>
        <table>
            <tr>
                <th>Name</th>
                <th>Litres Used</th>
            </tr>
            <% for(var i =0; i<data.length;i++) {%>
                <tr>
                    <td><%= data[i].name %></td>
                    <td><%= data[i].totalLitres %></td>
                </tr>
            <% } %>
        </table>
</body>
</html>
